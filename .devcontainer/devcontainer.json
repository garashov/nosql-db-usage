{
  "name": "Python Dev Container",
  "build": {
    "dockerfile": "Dockerfile"
  },
  "settings": {
    "python.pythonPath": "/usr/local/bin/python"
  },
  "extensions": [
    "ms-python.python",
    "ms-toolsai.jupyter"
  ],
  "forwardPorts": [
    8000
  ],
  "postCreateCommand": "poetry install --no-root --without dev",
  "remoteUser": "vscode"
}